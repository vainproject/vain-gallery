{
    "name": "vainproject/vain-gallery",
    "description": "This is a gallery module for the vain platform.",
    "license": "Apache-2.0",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "vainproject/vain": "dev-master",
        "vainproject/vain-user": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "orchestra/testbench": "^3.1.4"
    },
    "autoload": {
        "classmap": [
            "src/Gallery/Database"
        ],
        "psr-4": {
            "Modules\\Gallery\\": "src/Gallery/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Gallery\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
